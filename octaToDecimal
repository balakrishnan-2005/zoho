import java.util.*;
public class Main{
    static int octaToDecimal(int n){
        int res=0;
        int h=1;
        while(n>0){
            int k=n%10;
            res=res+(k*h);
            n=n/10;
            h=h*8;
        }
        return res;
    }
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int result=octaToDecimal(n);
        System.out.println(result);
    }
}
