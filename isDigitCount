import java.util.*;
public class Main{
static int isDigitCount(int n){
       int count = 0;
       while(n>0){
          count++;
          n=n/10;
       }
       return count;
    }
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int result=isDigitCount(n);
        System.out.println(result);
    }
}
