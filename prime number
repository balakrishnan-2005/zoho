import java.util.*;
public class Main{
static int isPrimeNumber(int n){
        int flag = 0;
        for(int i=2;i<=(n/2);i++){
           if(n%i==0){
             flag++;
             break;
           } 
        }
        return flag;
    }
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int res=isPrimeNumber(n);
        if(res==0){
            System.out.println("isPrimeNumber");
        }
        else{
            System.out.println("is not PrimeNumber");
        }
    }
}
